parallel
  seq
    interproc_1/tsar
    interproc_1/compile
  end_seq_seq
    loop_1/tsar
    loop_1/compile
  end_seq_seq
    loop_2/tsar
    loop_2/compile
  end_seq_seq
    Jacobi/tsar
    Jacobi/compile
  end_seq
end_parallel

